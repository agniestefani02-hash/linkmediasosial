  <script>
    const defaultConfig = {
      background_color: "#FFB6D9",
      button_color: "#FFFFFF",
      text_color: "#FF1493",
      page_title: "Media Sosial",
      instagram_text: "Instagram",
      instagram_url: "https://www.instagram.com/agniestefania_?igsh=MWIwZjlsYmJqZ3Bv&utm_source=qr",
      tiktok_text: "TikTok",
      tiktok_url: "https://www.tiktok.com/@agniestefani?_r=1&_t=ZS-91K7zkAI2Wv",
      youtube_text: "YouTube",
      youtube_url: "https://youtube.com/@nihongomantappu?si=tFoohm6Y7X8vCwQO",
      font_family: "Arial",
      font_size: 16
    };

    async function onConfigChange(config) {
      const backgroundColor = config.background_color || defaultConfig.background_color;
      const buttonColor = config.button_color || defaultConfig.button_color;
      const textColor = config.text_color || defaultConfig.text_color;
      const customFont = config.font_family || defaultConfig.font_family;
      const baseSize = config.font_size || defaultConfig.font_size;

      document.body.style.background = backgroundColor;
      document.body.style.fontFamily = `${customFont}, Arial, sans-serif`;

      const title = document.getElementById('pageTitle');
      title.textContent = config.page_title || defaultConfig.page_title;
      title.style.color = textColor;
      title.style.fontSize = `${baseSize * 2}px`;

      const links = document.querySelectorAll('.social-link');
      links.forEach(link => {
        link.style.backgroundColor = buttonColor;
        link.style.color = textColor;
        link.style.fontSize = `${baseSize * 1.125}px`;
      });

      const instagramLink = document.getElementById('instagramLink');
      const instagramText = document.getElementById('instagramText');
      instagramText.textContent = config.instagram_text || defaultConfig.instagram_text;
      instagramLink.href = config.instagram_url || defaultConfig.instagram_url;

      const tiktokLink = document.getElementById('tiktokLink');
      const tiktokText = document.getElementById('tiktokText');
      tiktokText.textContent = config.tiktok_text || defaultConfig.tiktok_text;
      tiktokLink.href = config.tiktok_url || defaultConfig.tiktok_url;

      const youtubeLink = document.getElementById('youtubeLink');
      const youtubeText = document.getElementById('youtubeText');
      youtubeText.textContent = config.youtube_text || defaultConfig.youtube_text;
      youtubeLink.href = config.youtube_url || defaultConfig.youtube_url;
    }

    function mapToCapabilities(config) {
      return {
        recolorables: [
          {
            get: () => config.background_color || defaultConfig.background_color,
            set: (value) => {
              if (window.elementSdk) {
                window.elementSdk.setConfig({ background_color: value });
              }
            }
          },
          {
            get: () => config.button_color || defaultConfig.button_color,
            set: (value) => {
              if (window.elementSdk) {
                window.elementSdk.setConfig({ button_color: value });
              }
            }
          },
          {
            get: () => config.text_color || defaultConfig.text_color,
            set: (value) => {
              if (window.elementSdk) {
                window.elementSdk.setConfig({ text_color: value });
              }
            }
          }
        ],
        borderables: [],
        fontEditable: {
          get: () => config.font_family || defaultConfig.font_family,
          set: (value) => {
            if (window.elementSdk) {
              window.elementSdk.setConfig({ font_family: value });
            }
          }
        },
        fontSizeable: {
          get: () => config.font_size || defaultConfig.font_size,
          set: (value) => {
            if (window.elementSdk) {
              window.elementSdk.setConfig({ font_size: value });
            }
          }
        }
      };
    }

    function mapToEditPanelValues(config) {
      return new Map([
        ["page_title", config.page_title || defaultConfig.page_title],
        ["instagram_text", config.instagram_text || defaultConfig.instagram_text],
        ["instagram_url", config.instagram_url || defaultConfig.instagram_url],
        ["tiktok_text", config.tiktok_text || defaultConfig.tiktok_text],
        ["tiktok_url", config.tiktok_url || defaultConfig.tiktok_url],
        ["youtube_text", config.youtube_text || defaultConfig.youtube_text],
        ["youtube_url", config.youtube_url || defaultConfig.youtube_url]
      ]);
    }

    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange,
        mapToCapabilities,
        mapToEditPanelValues
      });
    }
  </script>
